{
  "name": "wb-scripts",
  "version": "0.0.0-semantically-released",
  "description": "A CLI tool for WillBooster repos",
  "keywords": [
    "DevOps"
  ],
  "repository": "git@github.com:WillBooster/wb-scripts.git",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "yarn && babel-node -x .js,.jsx,.es6,.es,.ts -- src/main.ts",
    "start-prod": "yarn && yarn build && yarn gen-i18n-ts",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/node": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-numeric-separator": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@types/eslint": "7.28.2",
    "@types/jest": "27.0.2",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.11.6",
    "@types/semantic-release": "17.2.2",
    "@types/yargs": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@willbooster/eslint-config-ts": "6.4.2",
    "@willbooster/prettier-config": "6.4.0",
    "@willbooster/renovate-config": "6.4.2",
    "babel-jest": "27.3.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "core-js": "3.19.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.2",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "micromatch": "4.0.4",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "rollup": "2.59.0",
    "rollup-plugin-node-externals": "2.2.0",
    "semantic-release": "17.4.7",
    "sort-package-json": "1.52.0",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=12"
  },
  "packageManager": "yarn@3.1.0"
}
